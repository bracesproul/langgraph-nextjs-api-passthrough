{
  "name": "langgraph-nextjs-api-passthrough",
  "version": "0.0.4",
  "description": "A package which exports API route passthrough functions for using LangGraph from the client with Next.js",
  "packageManager": "yarn@1.22.22",
  "repository": {
    "type": "git",
    "url": "https://github.com/bracesproul/langgraph-nextjs-api-passthrough"
  },
  "homepage": "https://github.com/bracesproul/langgraph-nextjs-api-passthrough/blob/main/README.md",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Brace Sproul",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write .",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check ."
  },
  "peerDependencies": {
    "next": "*"
  },
  "devDependencies": {
    "next": "^15.2.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@tsconfig/recommended": "^1.0.7",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3"
  }
